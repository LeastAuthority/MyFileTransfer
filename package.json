{
  "name": "myFileTransfer",
  "version": "0.0.1",
  "private": true,
  "description": "An Ionic project",
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:worker": "vue-cli-service build --dest ./public/worker/ --target lib --formats umd --name worker --filename index --mode developement --watch ./src/worker.ts",
    "build": "NODE_ENV=production yarn build:worker && NODE_ENV=production vue-cli-service build",
    "build:worker": "NODE_ENV=production vue-cli-service build --dest ./dest/worker/ --target lib --formats umd --name worker --filename index --mode production ./src/worker.ts",
    "build:wasm": "node scripts/build-wasm.js",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "deploy:s3:playground": "aws s3 sync ./dist s3://wormhole.bryanchriswhite.com ",
    "deploy:s3:staging": "aws s3 sync ./dist s3://staging.transfer.leastauthority.com ",
    "deploy:s3:production": "aws s3 sync ./dist s3://transfer.leastauthority.com ",
    "compose": "bash scripts/docker-compose.sh",
    "compose:logs": "bash scripts/docker-compose.sh logs -f",
    "compose:up": "bash scripts/docker-compose.sh up"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.5",
    "@capacitor/core": "2.4.5",
    "@ionic/vue": "^5.4.0",
    "@ionic/vue-router": "^5.4.0",
    "core-js": "^3.6.5",
    "streamsaver": "^2.0.5",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-0",
    "web-streams-polyfill": "^3.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.5",
    "@types/jest": "^24.0.19",
    "@types/node": "^14.14.25",
    "@types/streamsaver": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "clipboardy": "^2.3.0",
    "cypress": "6.4.0",
    "cypress-file-upload": "^5.0.2",
    "cypress-wait-until": "^1.7.1",
    "cypress-waitfor": "^1.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "glob": "^7.1.6",
    "tmp-promise": "^3.0.2",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  }
}
